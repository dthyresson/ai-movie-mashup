{
  "name": "@redwoodjs/starter-standard",
  "version": "1.0.0",
  "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "pnpm build && pnpm vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "pnpm clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "pnpm clean && pnpm generate && pnpm build && wrangler deploy",
    "format": "prettier --write ./src",
    "generate": "npx prisma generate --no-hints",
    "migrate:dev": "pnpm generate && wrangler d1 migrations apply DB --local",
    "migrate:prd": "wrangler d1 migrations apply DB --remote",
    "migrate:new": "rw-scripts migrate-new",
    "seed": "pnpm worker:run ./src/scripts/seed.ts",
    "check": "pnpm generate && pnpm types",
    "types": "pnpm tsc"
  },
  "dependencies": {
    "@fontsource-variable/lexend": "5.2.6",
    "@fontsource/special-elite": "5.2.5",
    "@prisma/adapter-d1": "6.5.0",
    "@prisma/client": "6.5.0",
    "@redwoodjs/sdk": "0.0.57",
    "@simplewebauthn/browser": "13.1.0",
    "@simplewebauthn/server": "13.1.1",
    "@tailwindcss/vite": "4.1.4",
    "agents": "0.0.59",
    "ai": "4.3.7",
    "lucide-react": "0.487.0",
    "react": "19.0.0-rc-f2df5694-20240916",
    "react-dom": "19.0.0-rc-f2df5694-20240916",
    "react-server-dom-webpack": "19.0.0-rc-f2df5694-20240916",
    "tailwindcss": "4.1.4",
    "workers-ai-provider": "0.3.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "prisma": "6.5.0",
    "vite": "6.2.6",
    "wrangler": "4.11.0"
  },
